.search_bar {
  width: 100vw;
  height: 10vw;
  display: flex;
  align-items: center;
}
.search_bar .left_menu {
  width: 10vw;
  height: 10vw;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 8vw 8vw;
}
.search_bar .input_search_wrapper {
  width: 65vw;
  margin-left: 5vw;
  margin-right: 5vw;
  height: 9vw;
  border-radius: 10vw;
  border: 1px solid #000;
  padding-left: 2vw;
  display: flex;
}
.search_bar .input_search_wrapper .left_search_img {
  width: 9vw;
  height: 9vw;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 8vw 8vw;
}
.search_bar .input_search_wrapper input {
  height: 100%;
  width: 50vw;
}
.search_bar .mic {
  width: 10vw;
  height: 10vw;
  background: url(data:image/png;base64,) center center no-repeat;
  background-size: 8vw 8vw;
}

